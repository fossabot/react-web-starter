{
  "plugins": ["react"],
  "extends": [
    "standard",
    "standard-react",
    "plugin:react/recommended"
  ],
  "parserOptions": { "ecmaVersion": 6, "sourceType": "module", "ecmaFeatures": { "jsx": true } },
  "env": { "es6": true, "jest": true, "browser": true },
  "rules": {
    "no-console": "error",
    "global-require": "error"
  }
}
