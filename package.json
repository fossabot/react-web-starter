{
  "name": "react-web-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --open --config build/webpack.dev.js",
    "start:dist-server": "node tools/server/index.js",
    "start:mock": "node @mock_toolkit/index mock",
    "start:headless": "cross-env NODE_ENV=production webpack serve --config build/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.js",
    "test": "jest src --detectOpenHandles",
    "test:e2e": "jest test --detectOpenHandles --runInBand",
    "test:ci": "jest --detectOpenHandles --runInBand --coverage --ci",
    "coverage:upload": "codecov -f coverage/*.json",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "lint:css": "stylelint \"**/*.{css,scss,sass}\" --allow-empty-input",
    "lint:css:fix": "npm run lint:css -- --fix",
    "prettier": "prettier 'build/**' 'src/**' 'tools/**' --write --ignore-unknown",
    "prettier:ci": "prettier --check 'build/**' 'src/**' 'tools/**' --ignore-unknown",
    "prepare": "husky install",
    "postinstall": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "homepage": "https://github.com/vitorsalgado/react-web-starter/blob/master/README.md",
  "bugs": "https://github.com/vitorsalgado/react-web-starter/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:vitorsalgado/react-web-starter.git"
  },
  "author": {
    "name": "Vitor Hugo Salgado",
    "email": "vsalgadopb@gmail.com",
    "url": "https://github.com/vitorsalgado"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "tslib": "^2.1.0"
  },
