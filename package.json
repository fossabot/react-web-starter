    "@storybook/react": "^6.1.21",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.5",
    "@types/expect-puppeteer": "^4.4.5",
    "@types/jest": "^26.0.22",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/puppeteer": "^5.4.3",
