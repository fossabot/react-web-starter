  "scripts": {
    "build": "webpack --display-error-details --define process.env.NODE_ENV=\\\"'production'\\\" --config pkg/webpack.config.prod.js",
    "lint": "eslint ./src --ext js,jsx",
    "lint:fix": "npm run lint -- --fix",
    "lint:sonar": "node ./scripts/sonar.lint.js",
    "start": "webpack-dev-server --display-error-details --hot --open --config pkg/webpack.config.dev.js",
    "start:dist": "node tools/dist-dev-server.js",
    "start:mock": "node @mock_toolkit/index mock",
    "start:headless": "webpack-dev-server --config pkg/webpack.config.dev.js",
    "test": "jest src --detectOpenHandles",
    "test:ci": "npm test -- --ci --coverage --updateSnapshot",
    "test:update": "npm test -- --updateSnapshot",
    "test:e2e": "jest e2e --detectOpenHandles --runInBand",
    "test:e2e:ci": "npm run test:e2e -- --ci"
  },
